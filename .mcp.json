{
  "mcpServers": {
    "voice-multiplexer": {
      "command": "uv",
      "args": ["run", "--with", "fastmcp>=2.0", "--with", "websockets>=12.0", "${CLAUDE_PLUGIN_ROOT}/mcp-server/server.py"],
      "cwd": "${CLAUDE_PLUGIN_ROOT}",
      "env": {
        "RELAY_URL": "ws://localhost:3100"
      }
    }
  }
}
